{
  "name": "quiz-app-backend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/shreeraj-shinde/quiz-app-backend.git",
  "author": "jonasblues86 <jonasblues86@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "gen:types": "tsc -p tsconfig.types.json",
    "gen:types:watch": "tsc -p tsconfig.types.json -w",
    "start": "yarn prisma db push && yarn prisma generate --no-engine && node dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-model-validator": "^3.0.21",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "prisma": "^6.16.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./types": {
      "types": "./dist/types.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "types": [
        "dist/types.d.ts"
      ]
    }
  }
}
